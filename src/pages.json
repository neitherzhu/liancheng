{
  "easycom": {
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tab/home/index",
      "style": {
        "navigationBarTitleText": "专注力"
      }
    },
    {
      "path": "pages/tab/experience/index",
      "style": {
        "navigationBarTitleText": "练习"
      }
    },
    {
      "path": "pages/question-gen/index",
      "style": {
        "navigationBarTitleText": "题目生成"
      }
    },
    {
      "path": "pages/calculate-practice/index",
      "style": {
        "navigationBarTitleText": "视算练习"
      }
    },
    {
      "path": "pages/calculate-practice/question",
      "style": {
        "navigationBarTitleText": "视算练习",
        "pageOrientation": "landscape"
      }
    },
    {
      "path": "pages/pinyin-practice/index",
      "style": {
        "navigationBarTitleText": "拼音练习"
      }
    },
    {
      "path": "pages/pinyin-practice/question",
      "style": {
        "navigationBarTitleText": "拼音练习",
        "pageOrientation": "landscape"
      }
    },
    {
      "path": "pages/game/index",
      "style": {
        "navigationBarTitleText": "舒尔特方格"
      }
    }
  ],
